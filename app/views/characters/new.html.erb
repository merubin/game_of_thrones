<h1> Create a New Character in House <%=@house.name %> </h1>


<%= form_for [@house, @character] do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :portrayed_by %><br>
    <%= f.text_area :portrayed_by %>
  </p>

  <p>
    <%= f.label :img_url %><br>
    <%= f.url_field :img_url%>
  </p>

  <p>
    <%= f.label :seasons %><br>
    <%= f.text_area :seasons %>
  </p>

  <p>
    <%= f.label :first_seen %><br>
    <%= f.text_area :first_seen %>
  </p>

  <p>
    <%= f.label :status %><br>
    <%= f.text_area :status %>
  </p>


  <p>
    <%= f.submit %>
  </p>
<% end %>
