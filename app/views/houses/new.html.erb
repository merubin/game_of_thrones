<h1> Create a New House </h1>

<%= form_for @house do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :lord %><br>
    <%= f.text_field :lord %>
  </p>

  <p>
    <%= f.label :region %><br>
    <%= f.text_field :region%>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :img_url %><br>
    <%= f.url_field :img_url%>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
